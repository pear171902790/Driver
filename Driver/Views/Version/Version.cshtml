@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <style>
        * {
            margin: 0;
            padding: 0;
        }

        html, body {
            width: 100%;
            height: 100%;
        }
    </style>
</head>
<body>
<div style="margin: 200px auto; width: 50%;">
    <form method="POST" enctype="multipart/form-data">
        <div class="form-group">
            <label>Version Name</label>
            <input type="text" class="form-control" name="VersionName">
        </div>
        <div class="form-group">
            <label>Version Code</label>
            <input type="number" class="form-control" name="VersionCode">
        </div>
        <div class="form-group">
            <label>APK File</label>
            <input type="file" name="APK">
        </div>
        <button type="submit" class="btn btn-default">提交</button>
    </form>
    <div style="margin: 100px auto; width: 50%; font-size: 24px;">@(Request.QueryString["msg"])</div>
</div>
</body>
</html>